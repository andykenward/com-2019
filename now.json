{
  "version": 2,
  "name": "com-2019",
  "alias": "andykenward.com",
  "scope": "andykenward",
  "regions": ["lhr1"],
  "github": {
    "enabled": false,
    "autoAlias": false
  },
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build",
      "config": { "distDir": "public" }
    }
  ],
  "routes": [
    {
      "src": "^/(.*).html",
      "headers": { "cache-control": "public,max-age=0,must-revalidate" },
      "dest": "$1.html"
    }
  ]
}
